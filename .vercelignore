# Exclude large model files (these should be stored externally or downloaded at runtime)
backend/models/*.onnx
backend/models/*.pth
backend/models/*.pt
backend/models/**/*.onnx
backend/models/**/*.pth
backend/models/**/*.pt

# Exclude large node_modules packages (they're already in backend)
backend/node_modules/onnxruntime-node/**
backend/node_modules/@google-cloud/**
backend/node_modules/canvas/**
backend/node_modules/sharp/**

# Exclude data files (using Supabase now)
backend/data/images/**
backend/data/*.json

# Exclude Python files (using external service)
backend/models/*.py
backend/models/requirements.txt
backend/models/*.sh
backend/models/*.bat

# Exclude SQL migration files (not needed in production)
backend/*.sql
backend/*-guide.md
backend/*-commands.md
backend/*-required.md
backend/*-fix.md
backend/*-update.md
backend/*-steps.md
backend/*-guide.md
backend/create-admin-user.js

# Exclude development files
backend/node_modules/.cache
backend/.env.example
backend/.env.local
backend/.env.development

# Exclude test files
**/*.test.js
**/*.spec.js
**/__tests__/**
**/test/**

# Exclude documentation
*.md
!README.md

# Exclude Git files
.git
.gitignore
.gitattributes

# Exclude IDE files
.vscode
.idea
*.swp
*.swo
*~

# Exclude build artifacts
frontend/dist
frontend/.vite
backend/dist

# Exclude large backend dependencies that aren't needed in serverless function
# These packages will be available from backend/node_modules if needed
backend/node_modules/@google-cloud/speech
backend/node_modules/canvas
backend/node_modules/onnxruntime-node

# Exclude logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

